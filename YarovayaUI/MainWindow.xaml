<Window x:Class="YarovayaUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YarovayaUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button_load" Content="Button" HorizontalAlignment="Left" Margin="96,10,0,0" VerticalAlignment="Top" Width="74" Click="button_load_Click"/>
        <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="TextBlock" FontSize="14" Background="#FFDADADA" Margin="0,54,0,0"/>

    </Grid>
</Window>
